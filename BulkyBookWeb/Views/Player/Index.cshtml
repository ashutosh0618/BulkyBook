
@{
    ViewData["Title"] = "Index";
}
@model IEnumerable<Player>

<h1>Index Branch</h1>

<table class="table">
    <thead>
        <tr>
            <th>name</th>
            <th>age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
                    <tr>
                        <td>
                    @obj.Name
                        </td>
                        <td>
                    @obj.age
                        </td>
                    </tr>
        }
    </tbody>
</table>
