
@{
    ViewData["Title"] = "Index";
}
@model IEnumerable<Branch>

<h1>Index Branch</h1>

<table class="table">
    <thead>
        <tr>
            <th>name</th>
            <th>Type</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
                <tr>
                    <td>
                    @obj.Name
                    </td>
                    <td>
                    @obj.Type
                    </td>
                </tr>
        }
    </tbody>
</table>
